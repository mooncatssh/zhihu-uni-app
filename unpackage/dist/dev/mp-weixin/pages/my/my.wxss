.container {
  height: 100%;
  background-color: #ececec;
}
.container .content {
  padding-bottom: 10px;
  height: 130px;
  background-color: #ececec;
  display: flex;
  padding-top: 40px;
  flex-direction: column;
  align-items: center;
}
.container .content .titel {
  margin-top: 5px;
  font-size: 20px;
  color: #cccccc;
}
.container .border {
  background-color: white;
  padding-top: 10px;
}
.container .border .text1 {
  margin-left: 10px;
  font-size: 20px;
}
.container .border .text2 {
  margin-left: 10px;
  margin-top: 10px;
  font-size: 14px;
}
.container .border .text-box {
  margin-left: 20px;
  font-size: 12px;
}
.container .footer {
  height: 30px;
  background-color: white;
  padding: 0 10px;
  margin-top: 20px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .footer-last {
  margin-top: 10px;
  background-color: white;
}
.container .footer-last .text {
  margin-left: 10px;
}
.container .footer-last .text2 {
  font-size: 14px;
  width: 60%;
  margin-top: 25px;
  margin-left: 75px;
  height: 30px;
}
.container .footer-last .btn {
  margin-top: 10px;
  margin: 0 auto;
  width: 60%;
  height: 40px;
  border-radius: 20px;
  text-align: center;
  line-height: 40px;
  background-color: aqua;
  color: white;
}
.container .vue-ref {
  padding-top: 0;
  border-radius: 20px;
}
.container .vue-ref .login {
  border-radius: 20px;
  width: 90%;
  margin: 0 auto;
  height: 180px;
}
.container .vue-ref .login .login_titel {
  margin-top: 20px;
  margin-left: 20px;
  font-size: 25px;
}
.container .vue-ref .login .login_content {
  margin-top: 5px;
  margin-left: 20px;
  font-size: 13px;
  color: #949494;
}
.container .vue-ref .login .login_btn {
  margin-top: 50px;
  height: 50px;
  width: 100%;
  line-height: 50px;
  border-radius: 30px;
  text-align: center;
  background-color: #d9d9d9;
}
.container .vue-ref .login .login_rad {
  -webkit-transform: scale(0.7, 0.7);
          transform: scale(0.7, 0.7);
}
.container .vue-ref .login .login_foot {
  font-size: 10px;
  color: #949494;
}
.container .vue-ref .login .login_foot .cont {
  color: #5555ff;
}
.container .vue-ref .login .input {
  margin-left: 30px;
  width: 90%;
  height: 30px;
}
.container .vue-ref .login .input .uni-input1 {
  margin-top: 10px;
  height: 20px;
  border: 1px solid #e5e5e5;
}
.container .vue-ref .login .input .uni-input2 {
  margin-top: 10px;
  margin-bottom: 10px;
  height: 20px;
  border: 1px solid #e5e5e5;
}
.container .vue-ref .login .input .login_btns {
  width: 50%;
  margin: 0 auto;
  height: 28px;
  border-radius: 20px;
  text-align: center;
  line-height: 28px;
  background-color: #ffaaff;
  color: white;
}
.container .vue-ref .bootm {
  position: absolute;
  bottom: 0;
}

